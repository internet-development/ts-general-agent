{
  "name": "ts-general-agent",
  "description": "A long-running autonomous TypeScript agent",
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "version": "8.5.1",
  "type": "module",
  "scripts": {
    "agent": "tsx index.ts",
    "agent:walk": "tsx walk.ts",
    "agent:walk:quick": "tsx walk.ts --skip-improvement",
    "agent:reset": "tsx index.ts --flush-all",
    "agent:bluesky-reset": "tsx bluesky-reset.ts",
    "dev": "tsx watch index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@atproto/api": "^0.13.20",
    "ai": "^6.0.69",
    "dotenv": "^16.4.5",
    "playwright": "^1.58.1",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/sharp": "^0.31.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
